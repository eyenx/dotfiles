Config {
	font = "xft:liberation mono-8"
	, bgColor= "#3b3228"
	, fgColor= "#d0c8c6"
	, position = Top
    , pickBroadest = True
	, border = NoBorder
	, borderColor = "#3b3228"
	, lowerOnStart = True
	, commands = [
         Run Com "/home/eye/bin/gotify-check.py" ["count"] "gotify-check" 100,
         Run BatteryP ["BAT0","BAT1"] ["-t","<acstatus> <left>"
         ,"-L","10"
         ,"-H","70"
         ,"-l","#bb9584"
         ,"--"
         ,"-P"
         ,"-a","notify-send 'battery low'"
         ,"-i","<icon=/home/eye/.xmobar/xbm/ac1.xbm/>"
         ,"-O","<icon=/home/eye/.xmobar/xbm/ac1.xbm/>"
         ,"-o",""
         ,"-L","10"
         ,"-H","70"
         ,"--lows","<icon=/home/eye/.xmobar/xbm/batt1empty.xbm/> LOW BAT"
         ,"--mediums","<icon=/home/eye/.xmobar/xbm/batt1half.xbm/>"
         ,"--highs","<icon=/home/eye/.xmobar/xbm/batt1full.xbm/>"
         ] 100,
         Run Network "enp0s25" ["-t","<icon=/home/eye/.xmobar/xbm/net_wired.xbm/> <dev>"] 100,
         Run Wireless "wlp3s0" ["-t","<icon=/home/eye/.xmobar/xbm/wireless8.xbm/> <essid> <quality>%","-x","n/a"] 100,
         Run MultiCpu ["-t","<icon=/home/eye/.xmobar/xbm/cpu.xbm/> <total>%","-L","3","-H","80","--high","#bb9584"] 100,
         Run Mail [("<icon=/home/eye/.xmobar/xbm/mail.xbm/> a:", "~/.mail/work/INBOX")
                  ,("<icon=/home/eye/.xmobar/xbm/mail.xbm/> e:","~/.mail/eyenx/INBOX")
                  ,("<icon=/home/eye/.xmobar/xbm/mail.xbm/> p:","~/.mail/tauro/INBOX")] "mail",
         Run Memory ["--high","#bb9584","-t","<icon=/home/eye/.xmobar/xbm/mem1.xbm/> <usedratio>%"] 100,
         Run Date "<icon=/home/eye/.xmobar/xbm/clock2.xbm/> %H%M" "date" 300,
         Run Com "/home/eye/.conky/spacer.sh" [] "spacer" 10,
         Run Mpris2 "ncspot" ["-t","<icon=/home/eye/.xmobar/xbm/note1.xbm/> <artist> - <title>","-x",""] 100,
		 Run StdinReader
	]
	, sepChar = "%"
	, alignSep = "}{"
	, template = "%StdinReader% }{ %mpris2% %multicpu% %memory% %battery% %mail% <icon=/home/eye/.xmobar/xbm/messenger1.xbm/> %gotify-check% %enp0s25% %wlp3s0wi% %date% %spacer%"
