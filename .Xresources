! other urxvt options
URxvt.perl-ext: default,url-select
!,tabbed
URxvt*internalBorder:0
URxvt.urgentOnBell: true
!URxvt*tabbed.tabbar-bg:0
!URxvt*tabbed.tabbar-fg:8
!URxvt*tabbed.tab-bg:0
!URxvt*tabbed.tab-fg:8
URxvt*keysym.M-u: perl:url-select:select_next
!URxvt.url-select.launcher: /usr/bin/firefox -new-tab
URxvt.url-select.launcher: /usr/bin/chromium
URxvt.url-select.underline: true
URxvt*matcher.button: 1
URxvt*termName:screen-256color
!URxvt*termName:screen
!URxvt.font:xft:Liberation Mono for Powerline:size=8
URxvt.font:xft:Liberation Mono:size=8
!URxvt*letterSpace: 0 URxvt.saveLines:10000 URxvt.iconFile:/home/eye/.icons/ACYLBlue/scalable/apps/terminal.svg URxvt.geometry:70x35
URxvt.scrollBar: off
URxvt*cursorUnderline: 1
URxvt*underlineColor: #A3583B

! Xcursor -------------------------------------------------------------------

Xcursor.theme: aterred
Xcursor.size: 16


! xscreensaver --------------------------------------------------------------
!font settings
xscreensaver.Dialog.headingFont: xft:zekton:size=7
xscreensaver.Dialog.bodyFont: xft:zekton:size=7
xscreensaver.Dialog.labelFont: xft:zekton:size=7
xscreensaver.Dialog.unameFont: xft:zekton:size=7
xscreensaver.Dialog.buttonFont: xft:zekton:size=7
xscreensaver.Dialog.dateFont: xft:zekton:size=7
xscreensaver.passwd.passwdFont: xft:zekton:size=7
!general dialog box
xscreensaver.Dialog.foreground: #9e9c96
xscreensaver.Dialog.background: #1f1f1b
xscreensaver.Dialog.topShadowColor: #1f1f1b
xscreensaver.Dialog.bottomShadowColor: #1f1f1b
xscreensaver.Dialog.Button.foreground:  #9e9c96
xscreensaver.Dialog.Button.background:  #1f1f1b
!username/password input box and date text colour
xscreensaver.Dialog.text.foreground:    #9e9c96
xscreensaver.Dialog.text.background: #1f1f1b
xscreensaver.Dialog.borderWidth:        0
xscreensaver.Dialog.shadowThickness:    0
xscreensaver.passwd.thermometer.foreground: #1f1f1b
xscreensaver.passwd.thermometer.background: #1f1f1b
xscreensaver.passwd.thermometer.width: 0
!datestamp format--see the strftime(3) manual page for details
xscreensaver.dateFormat:    %H:%M - %a %d. %b %Y

Xft.dpi: 96
Xft.autohint: 0
Xft:lcdfilter: lcddefault
!Xft:lcdfilter: lcdlight
Xft.rgba: rgb
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.antialias: 1

